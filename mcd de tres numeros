// mcd de tres numeros.cpp : Defines the entry point for the console application.
// nombre:andres perez
// fecha : 22/04/14

#include "stdafx.h"
#include <iostream>
#include "conio.h"
using namespace std;
void main()
{int a,b,c,mcd,menor,cont;
cout<<"ingrese a";
cin>>a;
cout<<"ingrese b";
cin>>b;
cout<<"ingrese c";
cin>>c;
if(a<b&&a<c)
menor=a;
else
if(b<a&&b<c)
menor=b;
else
menor=c;
mcd=1;
cont=2;
while(cont<=menor)
{
	if(a%cont==0)&&(b%cont==0)&&(c%cont==0);
	{
		a=a/cont;
		b=b/cont;
		c=c/cont;
		menor=menor/cont;
		mcd=mcd*cont;
		cont=cont-1;
	}
	cont=cont+1;
}
cout<<mcd;
	getch();
}
