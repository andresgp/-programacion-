#include "stdafx.h"
#include <iostream>
#include "conio.h"


using namespace std;
void main()
{
int suma,i,Z;
cout<<"ingresar el valor de Z:";
cin>>Z;
suma=0;
cout <<"Suma=";
for (i=1;i<=Z;i=i+2)
{suma=suma+i;
cout<<i<<"+";}
cout<<"\n la suma de los "<<Z<<" primeros numeros enteros impares es:"<<suma;
    getch();
